{
  "name": "alpha-amqp-connection-manager",
  "version": "0.1.0",
  "description": "Connection manager for AMQP",
  "main": "compiled/index.js",
  "types": "compiled/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wookieb/alpha-amqp-connection-manager.git"
  },
  "author": "Łukasz Kużyński <lukasz.kuzynski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wookieb/alpha-amqp-connection-amanger/issues"
  },
  "keywords": [
    "amqp",
    "typescript",
    "connection",
    "reconnect"
  ],
  "engines": {
    "node": ">=4.3.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest --config jestConfig.json",
    "test:coverage": "npm test -- --coverage --no-cache",
    "test:ci": "npm test -- --testResultsProcessor jest-junit",
    "prepublish": "npm run compile",
    "compile": "npm run compile:all && rm -rf compiled/__tests__",
    "compile:all": "rm -rf compiled && ./node_modules/.bin/tsc"
  },
  "homepage": "https://github.com/wookieb/alpha-amqp-consumer#readme",
  "dependencies": {
    "amqplib": "0.5.*",
    "backoff": "2.*",
    "debug": "2.*",
    "@types/amqplib": "^0.5.1",
    "@types/debug": "0.0.29"
  },
  "devDependencies": {
    "@types/chai": "^3.5.0",
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.12",
    "@types/sinon": "^2.1.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "jest": "^20.0.0",
    "jest-junit": "^1.5.1",
    "mocha": "^3.1.2",
    "sinon": "^2.1.0",
    "ts-jest": "^20.0.4",
    "typescript": "^2.3.3"
  }
}
